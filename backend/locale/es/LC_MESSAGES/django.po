# Authentication
msgid "Password changed successfully"
msgstr "Contraseña cambiada exitosamente"

msgid "User does not belong to this tenant"
msgstr "El usuario no pertenece a este inquilino"

msgid "Logged out successfully"
msgstr "Sesión cerrada exitosamente"

msgid "Error during logout"
msgstr "Error durante el cierre de sesión"

msgid "You do not have permission to invite users"
msgstr "No tienes permiso para invitar usuarios"

msgid "Invalid or expired invitation"
msgstr "Invitación inválida o expirada"

msgid "Invitation has expired"
msgstr "La invitación ha expirado"

# Core
msgid "No tenant found"
msgstr "No se encontró inquilino"

msgid "Tenant activated successfully"
msgstr "Inquilino activado exitosamente"

msgid "Tenant deactivated successfully"
msgstr "Inquilino desactivado exitosamente"

msgid "Feature flag toggled successfully"
msgstr "Bandera de característica alternada exitosamente"

# Permissions
msgid "You must be a superuser to perform this action."
msgstr "Debes ser un superusuario para realizar esta acción."

msgid "You must be a tenant admin to perform this action."
msgstr "Debes ser un administrador de inquilino para realizar esta acción."

msgid "You must be a superuser or tenant admin to perform this action."
msgstr "Debes ser un superusuario o administrador de inquilino para realizar esta acción."

msgid "You must be a member of this tenant to perform this action."
msgstr "Debes ser miembro de este inquilino para realizar esta acción."

msgid "You must be the owner or a tenant admin to perform this action."
msgstr "Debes ser el propietario o un administrador de inquilino para realizar esta acción."

msgid "You do not have permission to manage users."
msgstr "No tienes permiso para gestionar usuarios."

msgid "You do not have permission to manage projects."
msgstr "No tienes permiso para gestionar proyectos."

msgid "You do not have permission to view audit logs."
msgstr "No tienes permiso para ver los registros de auditoría."

msgid "You do not have permission to modify this resource."
msgstr "No tienes permiso para modificar este recurso."

# Validation messages
msgid "Tenant with this slug already exists."
msgstr "Ya existe un inquilino con este slug."

msgid "Max users must be at least 1."
msgstr "El máximo de usuarios debe ser al menos 1."

msgid "Storage limit cannot be negative."
msgstr "El límite de almacenamiento no puede ser negativo."

msgid "Configuration with this key already exists."
msgstr "Ya existe una configuración con esta clave."

msgid "Configuration value cannot be empty."
msgstr "El valor de configuración no puede estar vacío."

msgid "Feature with this name already exists."
msgstr "Ya existe una característica con este nombre."

msgid "Rollout percentage must be between 0 and 100."
msgstr "El porcentaje de despliegue debe estar entre 0 y 100."

